{
  "expo": {
    "name": "ev-charging-station-app",
    "slug": "ev-charging-station-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.franclobo.evchargingstationapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "Outfit-Black": [
            "./assets/fonts/Outfit/static/Outfit-Black.ttf"
          ],
          "Outfit-Bold": [
            "./assets/fonts/Outfit/static/Outfit-Bold.ttf"
          ],
          "Outfit-ExtraBold": [
            "./assets/fonts/Outfit/static/Outfit-ExtraBold.ttf"
          ],
          "Outfit-ExtraLight": [
            "./assets/fonts/Outfit/static/Outfit-ExtraLight.ttf"
          ],
          "Outfit-Light": [
            "./assets/fonts/Outfit/static/Outfit-Light.ttf"
          ],
          "Outfit-Medium": [
            "./assets/fonts/Outfit/static/Outfit-Medium.ttf"
          ],
          "Outfit-Regular": [
            "./assets/fonts/Outfit/static/Outfit-Regular.ttf"
          ],
          "Outfit-SemiBold": [
            "./assets/fonts/Outfit/static/Outfit-SemiBold.ttf"
          ],
          "Outfit-Thin": [
            "./assets/fonts/Outfit/static/Outfit-Thin.ttf"
          ]
        }
      ],
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow EV Charging Station to use your location."
        }
      ]
    ],
    "scheme": "ev-charging-station-app",
    "extra": {
      "eas": {
        "projectId": "6c77b478-cd15-46f2-a684-743a5cd0c3e2"
      }
    },
    "owner": "franclobo"
  }
}
